{
  "name": "website-dvc",
  "version": "1.0.0",
  "description": "dataversioncontrol.com source code",
  "main": "index.js",
  "scripts": {
    "dev": "gatsby develop",
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "postbuild": "cp CNAME ./public && cp -r ./static ./public",
    "start": "cd ./docs && sudo python -m SimpleHTTPServer 80",
    "prettier": "prettier --no-semi --write './src/**/*.{js,jsx}'",
    "clean": "rm -rf ./.cache && rm -rf ./docs/* && rm -rf ./public/*",
    "precommit": "npm run prettier",
    "deploy": "npm run build && gh-pages -d public -b master"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0"
  },
  "dependencies": {
    "gatsby": "^1.9.235",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-google-analytics": "^1.0.24",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-nprogress": "^1.0.14",
    "gatsby-plugin-styled-components": "^2.0.9",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.37",
    "react": "latest",
    "react-ga": "^2.4.1",
    "react-sticky-el": "^1.0.16",
    "react-syntax-highlighter": "^7.0.2",
    "react-youtube": "^7.5.0",
    "styled-components": "^3.2.3",
    "styled-normalize": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dataversioncontrol/website-dvc.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dataversioncontrol/website-dvc/issues"
  },
  "homepage": "https://github.com/dataversioncontrol/website-dvc#readme"
}
